{
  "name": "ngcx-tree",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod:lib": "ng-packagr -p projects/ngcx-tree/ng-package.json -c projects/ngcx-tree/tsconfig.lib.prod.json",
    "publish:lib": "npm run copy:readme && npm run copy:changelog && npm run copy:license && npm publish ./dist/ngcx-tree --tag next",
    "prepare-publish:lib": "npm run copy:readme && npm run copy:changelog && npm run copy:license",
    "copy:readme": "cpx README.md dist/ngcx-tree",
    "copy:changelog": "cpx CHANGELOG.md dist/ngcx-tree",
    "copy:license": "cpx LICENSE dist/ngcx-tree",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "provide:link": "cd dist/ngcx-tree && npm link",
    "storybook": "ng run ngcx-tree:storybook --https --ssl-cert %AUDIT_DEV_ENVIRONMENT%/localenv/platform.mqa.crt --ssl-key %AUDIT_DEV_ENVIRONMENT%/localenv/platform.mqa.key",
    "build-storybook": "ng run ngcx-tree:build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/material": "^16.1.0",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.1",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular-eslint/template-parser": "^16.0.1",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "^16.1.0",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@storybook/addon-actions": "^7.3.1",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/angular": "^7.0.21",
    "@storybook/blocks": "^7.0.21",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/jest": "^29.5.2",
    "cpx2": "^5.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.5.0",
    "jest-preset-angular": "^13.1.1",
    "ng-packagr": "^16.1.0",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.21",
    "typescript": "~5.1.6"
  }
}
